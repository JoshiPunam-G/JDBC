package com.bridgelaz.clinic.model;

import java.util.Date;

public class Appointment {
	private int appid;
	private String appdate;
	private int  appnumber;
	private String time;
	private int appointmentcpount;

	public Appointment(int id, String date, int number, int count) {
		super();
		// TODO Auto-generated constructor stub
	}
	public Appointment(String appdate, String time, int appcpount, int appid, int appnumber, int appointmentcpount ) {
		super();
		this.appid = appid;
		this.appdate = appdate;
		this.appnumber = appnumber;
		this.time = time;
		this.appointmentcpount=appointmentcpount;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}

	@Override
	public String toString() {
		return "Appointment [appid=" + appid + ", appdate=" + appdate + ", appnumber=" + appnumber + ", time=" + time
				+ "]";
	}
	public int getAppid() {
		return appid;
	}
	public void setAppid(int appid) {
		this.appid = appid;
	}
	public String getAppdate() {
		return appdate;
	}
	public void setAppdate(String string) {
		this.appdate = string;
	}
	public int getAppnumber() {
		return appnumber;
	}
	public void setAppnumber(int appnumber) {
		this.appnumber = appnumber;
	}
//	public int getAppointmentcpount() {
//		return appointmentcpount;
//	}
//	public void setAppointmentcpount(int appointmentcpount) {
//		this.appointmentcpount = appointmentcpount;
//	}
	
	
	
	

}
